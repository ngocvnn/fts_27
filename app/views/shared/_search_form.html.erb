<%= search_form_for [:admin, @q] do |f| %>
  <%= f.condition_fields do |c| %>
    <%= c.attribute_fields do |a| %>
      <%= a.attribute_select %>
    <% end %>

    <%= c.predicate_select %>
    <%= c.value_fields do |v| %>
      <%= v.text_field :value %>
    <% end %>
  <% end %>

  <%= f.submit class: "col-md-3"%>
<% end %>  
